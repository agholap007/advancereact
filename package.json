{
  "name": "Basics",
  "version": "1.0.0",
  "description": "Advance react",
  "main": "lib/server.js",
  "repository": "https://github.com/agholap007/advancereact.git",
  "license": "MIT",
  "scripts": {
    "start": "set NODE_PATH=./lib&& nodemon --exec babel-node lib/server.js",
    "webpack": "webpack -wd",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"{,!(node_modules)/**/}*.js\"",
    "test": "jest --watchAll"
  },
  "prettier": "@imaginary-cloud/prettier-config",
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^7.8.1",
    "eslint-plugin-react": "7.19.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "react-test-renderer": "^16.13.1",
    "regenerator-runtime": "^0.13.7",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.0.0-0",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@imaginary-cloud/eslint-config-react": "1.0.1",
    "axios": "^0.20.0",
    "babel-loader": "^8.1.0",
    "ejs": "^3.1.5",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react-hooks": "2.5.1",
    "express": "^4.17.1",
    "pm2": "^4.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "webpack": "^4.44.1"
  }
}
